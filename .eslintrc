{
    "extends": "spatie",
    "env": {
        "browser": true,
        "node": true
    },
    "globals": {
        "$": true,
        "jQuery": true
    },
    "parserOptions": {
        "ecmaVersion": 2017
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "no-console": 0
    }
}
